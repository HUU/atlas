{
  "name": "prototype",
  "version": "0.0.0",
  "//": "devDependencies are for the whole monorepo",
  "devDependencies": {
    "@eslint/compat": "1.2.5",
    "@types/bcrypt": "5.0.0",
    "@types/node": "22.10.5",
    "cspell": "6.31.1",
    "depcheck": "1.4.3",
    "eslint": "9.18.0",
    "eslint-config-love": "116.0.0",
    "eslint-config-prettier": "10.0.1",
    "eslint-plugin-prettier": "5.2.2",
    "husky": "8.0.3",
    "markdownlint": "0.29.0",
    "markdownlint-cli2": "0.17.2",
    "prettier": "3.4.2",
    "prettier-plugin-jsdoc": "1.3.2",
    "prettier-plugin-organize-imports": "4.1.0",
    "syncpack": "13.0.0",
    "tsconfig-paths": "4.2.0",
    "turbo": "2.4.0",
    "typescript": "5.7.3",
    "typescript-eslint": "8.19.1",
    "vitest": "3.0.2"
  },
  "engines": {
    "node": ">=22.13.0"
  },
  "license": "UNLICENSED",
  "packageManager": "pnpm@10.2.1",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "clean": "turbo clean",
    "dev": "turbo watch dev --ui tui",
    "fix": "(syncpack format; e1=$?; pnpm turbo lint lint:root -- --fix; e2=$?; pnpm prettier --write '**/*.(yml|json)' .syncpackrc; e3=$?; [ $e1 = 0 ] && [ $e2 = 0 ] && [ $e3 = 0 ])",
    "lint": "turbo lint lint:json lint:markdown lint:spelling lint:syncpack lint:root",
    "lint:json": "prettier --check '**/*.(yml|json)' .syncpackrc",
    "lint:markdown": "markdownlint-cli2 '**/*.md' '#**/node_modules' '#**/.expo'",
    "lint:root": "eslint '*.{ts,js,mjs}' --max-warnings=0",
    "lint:spelling": "cspell . --gitignore --no-progress",
    "lint:syncpack": "syncpack lint",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install",
    "test": "turbo test"
  }
}
