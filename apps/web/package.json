{
  "name": "@atlas/web",
  "description": "Web app used by employees for managing client care and the API consumed by the mobile app.",
  "version": "0.0.0",
  "dependencies": {
    "@atlas/api": "workspace:*",
    "@atlas/common": "workspace:*",
    "@atlas/database": "workspace:*",
    "@dotenvx/dotenvx": "1.35.0",
    "@tanstack/react-router": "1.95.3",
    "@tanstack/start": "1.95.3",
    "@ts-rest/core": "3.51.0",
    "@ts-rest/serverless": "3.51.0",
    "adze": "2.2.0",
    "drizzle-orm": "0.38.4",
    "glob": "11.0.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "vinxi": "0.5.1",
    "zod": "3.24.1"
  },
  "devDependencies": {
    "@types/react": "19.0.4",
    "@types/react-dom": "19.0.2",
    "@vitejs/plugin-react": "4.3.4",
    "vitest": "3.0.2"
  },
  "private": true,
  "scripts": {
    "build": "vinxi build && cp -r ../../libraries/database/migrations .output/server",
    "clean": "rm -rf .output .vinxi",
    "depcheck": "depcheck .",
    "dev": "vinxi dev",
    "lint": "tsc && pnpm eslint \"src/**/*.{js,ts}\" \"*.ts\"",
    "set-secret": "dotenvx set",
    "start": "vinxi start",
    "test": "vitest run"
  },
  "type": "module"
}
