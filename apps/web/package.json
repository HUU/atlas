{
  "name": "@atlas/web",
  "description": "Web app used by employees for managing client care and the API consumed by the mobile app.",
  "version": "0.0.0",
  "dependencies": {
    "@atlas/api": "workspace:*",
    "@atlas/common": "workspace:*",
    "@atlas/database": "workspace:*",
    "@atlas/nitro": "workspace:*",
    "@dotenvx/dotenvx": "1.35.0",
    "@opentelemetry/api": "1.9.0",
    "@tanstack/react-query": "5.66.7",
    "@tanstack/react-query-devtools": "5.66.7",
    "@tanstack/react-router": "1.114.22",
    "@tanstack/react-router-devtools": "1.114.22",
    "@tanstack/react-router-with-query": "1.114.22",
    "@tanstack/react-start": "1.114.22",
    "@ts-rest/core": "3.52.1",
    "@ts-rest/react-query": "3.52.1",
    "@ts-rest/serverless": "3.52.1",
    "adze": "2.2.0",
    "autoprefixer": "10.4.20",
    "drizzle-orm": "0.38.4",
    "glob": "11.0.1",
    "pg": "8.13.1",
    "postcss": "8.5.2",
    "radashi": "12.3.4",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "7.54.2",
    "tailwindcss": "3.4.17",
    "vinxi": "0.5.1",
    "zod": "3.24.1"
  },
  "devDependencies": {
    "@atlas/infra": "workspace:*",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@vitejs/plugin-react": "4.3.4",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.0.2"
  },
  "private": true,
  "scripts": {
    "build": "vinxi build",
    "cd:build": "pnpm build",
    "check-config": "vitest run --reporter=dot \"__tests__/config.test.ts\"",
    "clean": "rm -rf .output .vinxi app.config.timestamp*",
    "depcheck": "depcheck .",
    "dev": "dotenvx run -f .env.development -- pnpm vinxi dev",
    "lint": "tsc && pnpm eslint \"src/**/*.{js,ts,tsx,jsx}\" \"*.{mjs,ts}\"",
    "set-secret": "dotenvx set",
    "start": "dotenvx run -f .env.development -- pnpm vinxi start",
    "test": "dotenvx run -f .env.development -- pnpm vitest run"
  },
  "type": "module"
}
